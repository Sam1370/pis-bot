{
    "name": "pis-bot",
    "version": "1.0.0",
    "description": "Support bot for the Pterodactyl Installation Script Discord.",
    "author": "sam1370",
    "license": "MIT",
    "main": "dist/index.js",
    "scripts": {
        "test": "tsc-watch --onCompilationComplete \"node dist/index.js\"",
        "start": "node dist/index.js",
        "build": "rimraf dist/* && tsc",
        "lint": "eslint \"src/**\"",
        "postinstall": "npm run build && node dist/setup.js",
        "pterodactyl": "git pull && yarn install --production && npm start"
    },
    "engines": {
        "node": ">=12"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/Sam1370/pis-bot.git"
    },
    "dependencies": {
        "@types/node": "^14.14.31",
        "@types/readline-sync": "^1.4.3",
        "@types/ws": "^7.4.0",
        "chalk": "^4.1.0",
        "discord.js": "^12.5.1",
        "moment": "^2.29.1",
        "readline-sync": "^1.4.10",
        "rimraf": "^3.0.2",
        "tesseract.js": "^2.1.4",
        "typescript": "^4.1.5"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.15.2",
        "@typescript-eslint/parser": "^4.15.2",
        "eslint": "^7.21.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "prettier": "^2.2.1",
        "tsc-watch": "^4.2.9"
    }
}
